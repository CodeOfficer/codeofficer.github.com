<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/images/favicon.png" rel="shortcut icon">
    <link href="/stylesheets/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/stylesheets/syntax.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/custom.css" media="screen" rel="stylesheet" type="text/css" />
  </head>
  <body>

    <header>
      <div class="row">
        <div class="span1">
        </div>
        <div class="span10">
          <div class="social right">
            <a class="google" href="https://plus.google.com/105488422714884256476" title="Google+">Google+</a>
            <a class="twitter" href="http://twitter.com/codeofficer" title="Twitter">Twitter</a>
            <a class="github" href="https://github.com/codeofficer" title="GitHub">GitHub</a>
            <a class="rss" href="/feed.xml" title="RSS">RSS</a>
          </div>

          <ul class="nav nav-pills">
            <% content_tag :li, :class => nav_active('index.html') do %>
              <%= link_to 'Home', '/index.html' %>
            <% end %>

            <% content_tag :li, :class => nav_active('archive.html') do %>
              <%= link_to 'Archive', '/archive.html' %>
            <% end %>
          </ul>
        </div>
        <div class="span1">
        </div>
      </div>
    </header>

    <div class="row">
      <div class="span1">
      </div>
      <div class="span10">
        <%= yield %>
      </div>
      <div class="span1">
      </div>
    </div>

    <script type="text/javascript">

      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-526530-11']);
      _gaq.push(['_trackPageview']);

      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();

    </script>

  </body>
</html>
