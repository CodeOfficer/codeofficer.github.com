<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/stylesheets/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="/stylesheets/syntax.css" media="screen" rel="stylesheet" type="text/css" />
  </head>
  <body>

    <div class="row">
      <div class="span1">
      </div>
      <div class="span10">
        <ul class="nav nav-pills">
          <% content_tag :li, :class => nav_active('index.html') do %>
            <%= link_to 'Home', '/index.html' %>
          <% end %>

          <% content_tag :li, :class => nav_active('archive.html') do %>
            <%= link_to 'Archive', '/archive.html' %>
          <% end %>
        </ul>
      </div>
      <div class="span1">
      </div>
    </div>

    <div class="row">
      <div class="span1">
      </div>
      <div class="span10">
        <%= yield %>
      </div>
      <div class="span1">
      </div>
    </div>

  </body>
</html>
